{
  "name": "search-prototype-simple",
  "version": "0.0.0",
  "scripts": {
    "dev:amsui": "ng build amsui --watch",
    "build:amsui": "ng build amsui --prod && npm run copy:all",
    "copy:styles": "ncp projects/amsui/src/styles dist/amsui/styles",
    "copy:translations": "ncp projects/amsui/src/translations dist/amsui/translations",
    "copy:all": "npm run copy:translations && npm run copy:styles",
    "patch:version": "cd projects/amsui && npm version patch",
    "deploy": "npm run build:amsui && npm run patch:version && npm publish ./dist/amsui --access public",
    "pack": "cd dist/amsui && npm pack",
    "ng": "ng",
    "start": "ng serve --hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s src/assets",
    "build-storybook": "npm run docs:json && build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.6",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.6",
    "@angular/compiler": "~11.0.6",
    "@angular/core": "~11.0.6",
    "@angular/forms": "~11.0.6",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.6",
    "@angular/platform-browser-dynamic": "~11.0.6",
    "@angular/router": "~11.0.6",
    "@larscom/ngx-translate-module-loader": "^2.1.1",
    "@ngx-translate/core": "^13.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "~11.0.6",
    "@babel/core": "^7.12.10",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/angular": "^6.1.21",
    "@storybook/storybook-deployer": "^2.8.7",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ncp": "^2.0.0",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "react": "^16.13.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
